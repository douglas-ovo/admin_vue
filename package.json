    "vite-plugin-mock": "^2.9.6",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@types/mockjs": "^1.0.6",
    "@vitejs/plugin-vue": "^2.3.2",
    "electron": "19.0.1",
    "electron-builder": "^23.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "nano-staged": "^0.8.0",
